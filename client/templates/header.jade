#header.container
  .logo-section.span3
  a(href='#')
    img#logo(src='./img/logo.png')
  .interface-section.span7
  .user-alerts
    ul.user-alerts-menu
    li.last
      img(src='./img/ico-message.png')
      a(href='#') Messages
    li
      img(src='./img/ico-notification.png')
      a(href='#') Notifications
  .user-ui
    ul.nav.pull-right
    li.dropdown
      a.dropdown-toggle(href='#', data-toggle='dropdown')
      i.caret
      ul.dropdown-menu.user-ui-menu
      li
        img(src='./img/ico-logout.png')
        a(href='#') Logout
      li
        img(src='./img/ico-settings.png')
        a(href='#') My Settings